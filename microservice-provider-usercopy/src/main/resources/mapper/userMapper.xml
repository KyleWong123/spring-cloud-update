<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.microserviceproviderusercopy.mapper.UserMapper">

    <select id="findAll" resultType="com.example.microserviceproviderusercopy.model.User">
        select * from user
    </select>
    <select id="findByName" resultType="com.example.microserviceproviderusercopy.model.User">
        select * from user where username like concat('%',replace(#{username},'%','\%'),'%')
    </select>
</mapper>
